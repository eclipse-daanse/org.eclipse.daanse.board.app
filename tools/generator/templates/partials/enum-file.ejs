<%#
  Enum file template - creates separate .ts file for each enum
_%>
<%- `/* File: ${eEnum.name}.ts */` -%>

export enum <%- eEnum.name -%> {
<%_ if (eEnum.eLiterals) { _%>
  <%_ Array.from(eEnum.eLiterals).forEach((literal, index) => { _%>
    <%- literal.literal -%> = "<%- literal.literal -%>"<%_ if (index < eEnum.eLiterals.length - 1) { _%>,<%_ } _%>
  <%_ }); _%>
<%_ } _%>
}