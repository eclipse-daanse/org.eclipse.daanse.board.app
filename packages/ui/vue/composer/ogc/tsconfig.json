{
  "compilerOptions": {
    "sourceMap": true,
    "declarationMap": true,
    "inlineSources": true,
    "target": "ESNext"
,
    "experimentalDecorators": true
,
    "emitDecoratorMetadata": true
,
    "module": "commonjs"
,
    "declaration": true
,
    "outDir": "./dist/types"
,
    "esModuleInterop": true
,
    "forceConsistentCasingInFileNames": true
,
    "strict": true
,
    "skipLibCheck": true
  }
}
